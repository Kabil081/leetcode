class Solution{
    public char findTheDifference(String s, String t){
        if(s.length()==0) return t.charAt(0);
        int freq[]=new int[26];
        for(char ch:s.toCharArray()) freq[ch-'a']++;
        for(char ch:t.toCharArray()){ 
            if(freq[ch-'a']==0) return ch;
            freq[ch-'a']--;
        }
        return 'a';
    }
}
