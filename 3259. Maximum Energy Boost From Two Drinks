class Solution{
    public long maxEnergyBoost(int[] drinkA,int[] drinkB){
        long maxA=0;
        long maxB=0;
        for(int i=0;i<drinkA.length;i++){
            long newMaxA=Math.max(maxB,maxA+drinkA[i]);
            long newMaxB=Math.max(maxA,maxB+drinkB[i]);
            maxA=newMaxA;
            maxB=newMaxB;
        }
        return Math.max(maxA,maxB);
    }
}
