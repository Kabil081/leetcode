class Solution{
    public long maximumScore(int[] nums){
        int n=nums.length;
        int suffix_min[]=new int[n];
        suffix_min[n-1]=nums[n-1];
        for(int i=n-2;i>0;i--){
            suffix_min[i]=Math.min(suffix_min[i+1],nums[i]);
        }
        long ans=Integer.MIN_VALUE;
        long prefix_sum=0;
        for(int i=0;i<n-1;i++){
            prefix_sum+=nums[i];
            ans=Math.max(ans,prefix_sum-suffix_min[i+1]);
        }
        return ans;
    }
}
