class Solution {
    public int countMajoritySubarrays(int[] nums, int target) {
        int res=0;
        for(int i=0;i<nums.length;i++){
            int cur=0;
            if(nums[i]==target){
                res++;
                cur++;
            }
            for(int j=i+1;j<nums.length;j++){
                if(nums[j]==target) cur++;
                if(cur>(j-i+1)/2) res++;
            }
        }
        return res;
    }
}
