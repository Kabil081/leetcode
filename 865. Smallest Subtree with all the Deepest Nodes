/**
 * Definition for a binary tree node.
 * public class TreeNode {
 *     int val;
 *     TreeNode left;
 *     TreeNode right;
 *     TreeNode() {}
 *     TreeNode(int val) { this.val = val; }
 *     TreeNode(int val, TreeNode left, TreeNode right) {
 *         this.val = val;
 *         this.left = left;
 *         this.right = right;
 *     }
 * }
 */
class Solution{
    int depth;
    public TreeNode subtreeWithAllDeepest(TreeNode root) {
        depth=find_depth(root);
        return helper(root,1);
    }
    public int find_depth(TreeNode root){
        if(root==null) return 0;
        return Math.max(find_depth(root.left),find_depth(root.right))+1;
    }
    public TreeNode helper(TreeNode root,int cur){
        if(cur==depth || root==null) return root;
        TreeNode left=helper(root.left,cur+1);
        TreeNode right=helper(root.right,cur+1);
        if(left!=null && right!=null) return root;
        return left!=null?left:right;
    }
}
