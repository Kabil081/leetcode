class Solution{
    public int numberOfPairs(int[][] points){
        int count=0;
        Arrays.sort(points,(a,b)->{
            if(a[0]==b[0]){
                return b[1]-a[1];
            }
            return a[0]-b[0];
        });
        for(int i=0;i<points.length;i++){
            int y_i=points[i][1];
            int min_y=Integer.MIN_VALUE;
            for(int j=i+1;j<points.length;j++){
                int y_j=points[j][1];
                if(y_j>y_i) continue;
                if(y_j>min_y){
                    min_y=y_j;
                    count++;
                }
            }
        }
        return count;
    }
}
