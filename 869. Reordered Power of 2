class Solution{
    public boolean reorderedPowerOf2(int n){
        int f[]=new int[10];
        int digits=0;
        while(n>0){
            f[n%10]++;
            n/=10;
            digits++;
        }
        for(int i=0;i<30;i++){
            int freq[]=new int[10];
            int num=(int)Math.pow(2,i);
            int curDigits=0;
            while(num>0){
                freq[num%10]++;
                num/=10;
                curDigits++;
            }
            if(curDigits>digits) return false;
            if(Arrays.equals(f,freq)) {
                return true;
            }
        }
        return false;
    }
}
