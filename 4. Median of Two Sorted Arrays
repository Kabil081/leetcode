double findMedianSortedArrays(int* nums1, int nums1Size, int* nums2, int nums2Size) 
{
    double result=0;
    int *arr=(int*)malloc((nums1Size+nums2Size)*sizeof(int)),i=0,j=0,k=0;
    while(i<nums1Size && j<nums2Size)
    {
        if(nums1[i]<nums2[j])
        {
            arr[k++]=nums1[i++];
        }
        else
        {
            arr[k++]=nums2[j++];
        }
    }
    while(i<nums1Size)
    {
        arr[k++]=nums1[i++];
    }
    while(j<nums2Size)
    {
        arr[k++]=nums2[j++];
    }
    if(k%2==0)
    {
        int m=k/2;
        result+=arr[m]+arr[m-1];
        result=result/2;
    }
    else
    {
        result+=arr[k/2];
    }
    return result;
}
