class MyStack{
    Queue<Integer> queue;
    public MyStack(){
        queue=new LinkedList<>();
    }
    public void push(int x){
        Queue<Integer> q=new LinkedList<>();
        while(!queue.isEmpty()) q.add(queue.poll());
        queue.add(x);
        while(!q.isEmpty()) queue.add(q.poll());
    }
    public int pop(){
        return queue.poll();
    }
    public int top(){
        return queue.peek();
    }
    public boolean empty(){
        return queue.isEmpty();
    }
}
