class Solution{
    public String largestGoodInteger(String num){
        String ans="";
        for(int i=0;i<=num.length()-3;i++){
            if(num.charAt(i)!=num.charAt(i+1) || num.charAt(i)!=num.charAt(i+2)) continue;
            String cur=num.substring(i,i+3);
            if(ans.compareTo(cur)<0) ans=cur;
        }
        return ans;
    }
}
