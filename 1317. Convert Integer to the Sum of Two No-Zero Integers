class Solution{
    public int[] getNoZeroIntegers(int n){
        int left=n/2,right=n/2+n%2;
        while(left+right==n){
            if(isTrue(left) && isTrue(right)) return new int[]{left,right};
            left--;
            right++;
        }
        return new int[]{};
    }
    public boolean isTrue(int n){
        while(n>0){
            int rem=n%10;
            if(rem==0) return false;
            n/=10;
        }
        return true;
    }
}
