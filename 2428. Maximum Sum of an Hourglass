class Solution{
    public int maxSum(int[][] grid){
        int m=grid.length;
        int n=grid[0].length;
        int ans=0;
        for(int i=0;i<m-2;i++){
            for(int j=0;j<n-2;j++){
                int cur=0;
                for(int k=i;k<i+3;k+=2){
                    for(int l=j;l<j+3;l++){
                        cur+=grid[k][l];
                    }
                }
                cur+=grid[i+1][j+1];
                ans=Math.max(ans,cur);
            }
        }
        return ans;
    }
}
