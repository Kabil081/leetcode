class Solution{
    public int longestSubsequence(int[] nums){
        int ans=0;
        for(int i:nums){
            ans^=i;
        }
        int same=0;
        for(int i=0;i<nums.length;i++){
            if(nums[i]==0) same++;
        }
        if(same==nums.length) return 0;
        return ans==0?nums.length-1:nums.length;
    }
}
