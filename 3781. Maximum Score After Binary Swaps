class Solution{
    public long maximumScore(int[] nums, String s){
        long res=0;
        PriorityQueue<Integer> pq=new PriorityQueue<>((a,b)->b-a);
        for(int i=0;i<s.length();i++){
            pq.add(nums[i]);
            if(s.charAt(i)=='1'){
                res+=pq.poll();
            }
        }
        return res;
    }
}
