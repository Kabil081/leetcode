class Solution {
    public boolean canConstruct(String s1, String s2) {
        int freq[]=new int[26];
        for(char ch:s2.toCharArray()) freq[ch-'a']++;
        for(char ch:s1.toCharArray()){
            freq[ch-'a']--;
            if(freq[ch-'a']<0) return false;
        }
        return true;
    }
}
