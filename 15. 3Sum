class Solution {
    public List<List<Integer>> threeSum(int[] nums) {
        List<List<Integer>> res=new ArrayList<>();
        Arrays.sort(nums);
        for(int i=0;i<nums.length-2;i++){
            if(i>0 && nums[i]==nums[i-1]) continue;
            int start=i+1;
            int end=nums.length-1;
            int target=0-nums[i];
            while(start<end){
                int cur=nums[start]+nums[end];
                if(cur<target){
                    start++;
                }else if(cur>target){
                    end--;
                }else{
                    List<Integer> list=new ArrayList<>();
                    list.add(nums[i]);
                    list.add(nums[start]);
                    list.add(nums[end]);
                    res.add(new ArrayList<>(list));
                    int num=nums[end];
                    while(end>start && nums[end]==num) end--;
                    num=nums[start];
                    while(start<end && nums[start]==num) start++;
                }
            }
        }
        return res;
    }
}
