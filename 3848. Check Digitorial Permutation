class Solution{
    public boolean isDigitorialPermutation(int n){
        int freq1[]=new int[10];
        int freq2[]=new int[10];
        int num=n;
        int sum=0;
        while(num!=0){
            int rem=num%10;
            freq1[rem]++;
            sum+=fact(rem);
            num/=10;
        }
        while(sum!=0){
            int rem=sum%10;
            freq2[rem]++;
            sum/=10;
        }
        return Arrays.equals(freq1,freq2);
    }
    public int fact(int n){
        if(n==0 || n==1) return 1;
        return n*fact(n-1);
    }
}
