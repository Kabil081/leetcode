class Solution {
    public int maxArea(int[] height) {
        int ans=0;
        int l=0,r=height.length-1;
        while(l<r && r>0){
            int cur=Math.min(height[l],height[r]);
            ans=Math.max(ans,cur*(r-l));
            if(height[l]<height[r]) l++;
            else r--;
        }
        return ans;
    }
}
