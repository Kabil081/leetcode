class Solution{
    public int[] getSumAbsoluteDifferences(int[] nums){
        int n=nums.length;
        int prefix[]=new int[n+1];
        int result[]=new int[n];
        int totalSum=0;
        for(int i=0;i<n;i++){
            totalSum+=nums[i];
            prefix[i+1]=prefix[i]+nums[i];
        }
        for(int i=0;i<n;i++){
            result[i]=((nums[i]*i)-prefix[i])+((totalSum-prefix[i+1])-(nums[i]*(n-i-1)));
        }
        return result;
    }
}
