class Solution {
    public String maximumXor(String s, String t) {
        int freq[]=new int[2];
        for(char ch:t.toCharArray()){
            freq[ch-'0']++;
        }
        StringBuilder sb=new StringBuilder();
        for(char ch:s.toCharArray()){
            int ind=ch-'0';
            if(ind==0){
                if(freq[1]>0){
                    sb.append('1');
                    freq[1]--;
                }else{
                    sb.append('0');
                    freq[0]--;
                }
            }else{
                if(freq[0]>0){
                    sb.append('1');
                    freq[0]--;
                }else{
                    sb.append('0');
                    freq[1]--;
                }
            }
        }
        return sb.toString();
    }
}
