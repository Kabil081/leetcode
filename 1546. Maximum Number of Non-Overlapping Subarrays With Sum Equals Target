class Solution{
    public int maxNonOverlapping(int[] nums, int target){
        HashSet<Integer> set=new HashSet<>();
        set.add(0);
        int sum=0;
        int ans=0;
        for(int i:nums){
            sum+=i;
            if(set.contains(sum-target)){
                ans++;
                set.clear();
            }
            set.add(sum);
        }
        return ans;
    }
}
