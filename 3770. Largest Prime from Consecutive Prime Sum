class Solution{
    boolean is_prime[];
    public int largestPrime(int n){
        if(n==2 || n==3) return 2;
        return prime_range(n);
    }
    public int prime_range(int n){
        is_prime=new boolean[n+1];
        Arrays.fill(is_prime,true);
        is_prime[0]=false;
        is_prime[1]=true;
        int count=2;
        for(int i=2;i*i<=n;i++){
            if(!is_prime[i]) continue;
            for(int j=i+i;j<=n;j+=i){
                if(is_prime[j]) count++;
                is_prime[j]=false;
            }
        }
        int sum=0;
        int max=0;
        for(int i=2;i<n;i++){
            if(is_prime[i]){ 
                sum+=i;
                if(sum>n) return max;
                if(is_prime[sum]) max=Math.max(max,sum);
            }
        }
        return max;
    }
}
