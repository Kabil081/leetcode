class Solution{
    public int[] findXSum(int[] nums, int k, int x){
        int n=nums.length;
        int res[]=new int[n-k+1];
        for(int i=0;i<=n-k;i++){
            Set<Integer> unique=new HashSet<>();
            PriorityQueue<int[]> pq=new PriorityQueue<>((a,b)->{
                if(a[0]==b[0]) return b[1]-a[1];
                return b[0]-a[0];
            });
            int freq[]=new int[51];
            int sum=0;
            for(int j=i;j<i+k;j++){
                unique.add(nums[j]);
                freq[nums[j]]++;
                sum+=nums[j];
            }
            if(unique.size()<x) res[i]=sum;
            else{
                for(int num:unique){
                    pq.add(new int[]{freq[num],num});
                }
                int itr=0;
                while(itr<x){
                    res[i]+=pq.peek()[0]*pq.poll()[1];
                    itr++;
                }
            }
        }
        return res;
    }
}
