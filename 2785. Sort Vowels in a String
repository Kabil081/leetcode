class Solution{
    public String sortVowels(String s) {
        char []vowel=new char[s.length()];
        Arrays.fill(vowel,'z');
        int ind=0;
        String vowels="aeiouAEIOU";
        for(char ch:s.toCharArray()){
            if(vowels.contains(ch+"")){
                vowel[ind]=ch;
                ind++;
            }
        }
        Arrays.sort(vowel);
        ind=0;
        StringBuilder sb=new StringBuilder();
        for(char ch:s.toCharArray()){
            if(vowels.contains(ch+"")){ 
                sb.append(vowel[ind]);
                ind++;
            }else sb.append(ch);
        }
        return sb.toString();
    }
}
