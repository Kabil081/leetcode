class Solution{
    public int[] successfulPairs(int[] spells, int[] potions, long success){
        Arrays.sort(potions);
        int n=spells.length;
        int m=potions.length;
        int result[]=new int[n];
        for(int i=0;i<n;i++){
            int ind=bsearch(0,m-1,potions,success,spells[i]);
            if(ind!=-1) result[i]=m-ind;
        }
        return result;
    }
    public int bsearch(int left,int right,int arr[],long success,long spell){
        int ans=-1;
        while(left<=right){
            int mid=left+(right-left)/2;
            long val=arr[mid]*spell;
            if(val>=success){
                ans=mid;
                right=mid-1;
            }else{
                left=mid+1;
            }
        }
        return ans;
    }
}
