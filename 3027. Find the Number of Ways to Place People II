class Solution {
    public int numberOfPairs(int[][] points) {
        int count=0;
        Arrays.sort(points,(a,b)->{
            if(a[0]==b[0]) return b[1]-a[1];
            return a[0]-b[0];
        });
        for(int i=0;i<points.length;i++){
            int y=points[i][1];
            int min_y=Integer.MIN_VALUE;
            for(int j=i+1;j<points.length;j++){
                if(y<points[j][1]) continue;
                if(points[j][1]>min_y){
                    min_y=points[j][1];
                    count++;
                }
            }
        }
        return count;
    }
}
