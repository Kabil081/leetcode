class Solution{
    public int sortPermutation(int[] nums) {
        int min=-1;
        for(int i=0;i<nums.length;i++){
            if(nums[i]!=i) {
                if(min==-1){
                    min=nums[i];
                }else{
                    min=min & nums[i];
                }
            }
        }
        return min==-1?0:min;
    }
}
