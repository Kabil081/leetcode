class Solution {
    public int maxProfit(int k, int[] prices) {
        int n=prices.length;
        int dp[][][]=new int[n+1][2][k+1];
        for(int ind=n-1;ind>=0;ind--){
            for(int buy=0;buy<=1;buy++){
                for(int transactions=1;transactions<=k;transactions++){
                    if(buy==1){
                        dp[ind][buy][transactions]=Math.max(-prices[ind]+dp[ind+1][0][transactions],dp[ind+1][1][transactions]);
                    }else{
                        dp[ind][buy][transactions]=Math.max(prices[ind]+dp[ind+1][1][transactions-1],dp[ind+1][0][transactions]);
                    }
                }
            }
        }
        return dp[0][1][k];
    }
}
