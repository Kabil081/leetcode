class Solution {
    public int[] maxKDistinct(int[] nums, int k) {
        Map<Integer,Integer> map=new HashMap<>();
        List<Integer> list=new ArrayList<>();
        Arrays.sort(nums);
        int ind=nums.length-1;
        while(k>0 && ind>=0){
            int num=nums[ind];
            if(map.containsKey(num)){ 
                ind--;
                continue;
            }
            map.put(num,1);
            list.add(num);
            k--;
            ind--;
        }
        int arr[]=new int[list.size()];
        for(int i=0;i<list.size();i++){
            arr[i]=list.get(i);
        }
        return arr;
    }
}
