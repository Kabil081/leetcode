class Solution {
    public boolean partitionArray(int[] nums, int k) {
        if(nums.length%k!=0) return false;
        int groups=nums.length/k;
        int freq[]=new int[100001];
        for(int i:nums){
            freq[i]++;
            if(freq[i]>groups) return false;
        }
        return true;
    }
}
