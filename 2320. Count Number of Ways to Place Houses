class Solution{
    public int countHousePlacements(int n){
        if(n==1 || n==2) return (n+1)*(n+1);
        long m = 1000000007;
        long start=2,next=3;
        for(int i=3;i<=n;i++){
            long temp=start;
            start=next;
            next=(temp+next)%m;
        }
        return (int)((next*next)%m); 
    }
}
