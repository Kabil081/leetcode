class Solution{
    public int minTimeToVisitAllPoints(int[][] points){
        int count=0;
        for(int i=1;i<points.length;i++){
            int x=points[i][0];
            int y=points[i][1];
            int prev_x=points[i-1][0];
            int prev_y=points[i-1][1];
            count+=Math.max(Math.abs(x-prev_x),Math.abs(y-prev_y));
        }
        return count;
    }
}
